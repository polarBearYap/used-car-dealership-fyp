<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="author" content="Yap Jheng Khin" />
    <title>@ViewData["Title"] - Car Dealership</title>
    <!-- ============================================================== -->
    <!-- Import dependencies & libraries -->
    <!-- ============================================================== -->
    <!-- <script src="~/lib/jquery/jquery.min.js" type="text/javascript"></script> -->
    <!-- <script src="~/lib/twitter-bootstrap/js/bootstrap.bundle.min.js" type="text/javascript"></script> -->
    <link rel="stylesheet" href="~/lib/twitter-bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/font/font-awesome/css/font-awesome.min.css" />
    <!-- ============================================================== -->
    <!-- End dependencies & libraries -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Custom css files -->
    <!-- ============================================================== -->
    <link rel="stylesheet" href="~/css/default_style.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <!-- ============================================================== -->
    <!-- End custom css files -->
    <!-- ============================================================== -->
</head>
<body class="container sb-nav-fixed">
    <div id="loading" class="outer" style="display: table;">
        <div class="middle">
            <div class="inner">
                <div class="spinner-border text-primary" role="status">
                    <span class="visually-hidden">Loading...</span>
                </div>
            </div>
        </div>
    </div>
    <div id="page">
        <nav class="row justify-content-between align-items-center sb-topnav navbar navbar-expand navbar-dark color-primary-dark">
            <!-- Navbar Brand-->
            <div class="col-8 d-flex justify-content-start align-items-center" style="max-width: 100%; max-height: 100%;">
                <a class="d-flex justify-content-start align-items-center navbar-brand pl-2" style="display: inline-block;"
                   asp-area="" asp-controller="Home" asp-action="Index">
                    <img src="~/logo/logo_primary.png" style="width: 40px; height: 40px;" />
                    &nbsp;&nbsp;Honk Honk
                </a>
                <!-- Sidebar Toggle -->
                <button class="float-left text-decoration-none btn btn-link btn-sm order-1 order-lg-0 me-4 me-lg-0" id="sidebarToggle" href="#">
                    <i class="fa fa-bars"></i>
                    &nbsp;&nbsp;Menu
                </button>
            </div>
            <!-- Navbar-->
            <div class='col-4 d-flex justify-content-end align-items-center'>
                <ul class="navbar-nav ms-auto ms-md-0 me-3 me-lg-4">
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" id="navbarDropdown" href="#" role="button" data-bs-toggle="dropdown"
                           aria-expanded="false"><i class="fa fa-user fa-fw"></i></a>
                        <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdown">
                            <li><a class="dropdown-item" href="#">Settings</a></li>
                            <li><a class="dropdown-item" href="#">Activity Log</a></li>
                            <li>
                                <hr class="dropdown-divider" />
                            </li>
                            <li><a class="dropdown-item" href="#">Logout</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>
        <div id="layoutSidenav">
            <div id="layoutSidenav_nav">
                <nav class="sb-sidenav accordion sb-sidenav-light color-primary" id="sidenavAccordion">
                    <div class="sb-sidenav-menu">
                        <div class="nav">
                            <div class="sb-sidenav-menu-heading">Inventories</div>
                            <a class="nav-link collapsed" href="#"
                               data-bs-toggle="collapse" data-bs-target="#collapseVehicle" aria-expanded="false" aria-controls="collapseVehicle">
                                <div class="sb-nav-link-icon"><i class="fa fa-car"></i></div>
                                Car Inventory
                                <div class="sb-sidenav-collapse-arrow"><i class="fa fa-angle-down"></i></div>
                            </a>
                            <div class="collapse" id="collapseVehicle" aria-labelledby="headingOne" data-bs-parent="#sidenavAccordion">
                                <nav class="sb-sidenav-menu-nested nav">
                                    <a class="nav-link" asp-area="" asp-controller="Cars" asp-action="Index">Car</a>
                                </nav>
                            </div>
                            <a class="nav-link collapsed" href="#"
                               data-bs-toggle="collapse" data-bs-target="#collapseLead" aria-expanded="false" aria-controls="collapseLead">
                                <div class="sb-nav-link-icon"><i class="fa fa-address-book"></i></div>
                                Lead Management
                                <div class="sb-sidenav-collapse-arrow"><i class="fa fa-angle-down"></i></div>
                            </a>
                            <div class="collapse" id="collapseLead" aria-labelledby="headingOne" data-bs-parent="#sidenavAccordion">
                                <nav class="sb-sidenav-menu-nested nav">
                                    <a class="nav-link" asp-area="" asp-controller="Leads" asp-action="Index">Lead</a>
                                </nav>
                            </div>
                            <!--<div class="sb-sidenav-menu-heading">Accounts</div>-->
                            <!--
                            <a class="nav-link" href="#">
                                <div class="sb-nav-link-icon"><i class="fa fa-users"></i></div>
                                Manage Users
                            </a>
                            -->
                        </div>
                    </div>
                </nav>
            </div>
            <div id="layoutSidenav_content">
                <div class="container">
                    <main>
                        @RenderBody()
                    </main>
                </div>
                <footer class="bg-light mt-auto border-top footer text-muted">
                    <div class="container-fluid px-4">
                        <div class="d-flex align-items-center justify-content-between small">
                            <span class="text-muted">&copy; 2021 - Car Dealership</span> - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </div>
                    </div>
                </footer>
                @await RenderSectionAsync("Scripts", required: false)
            </div>
        </div>
    </div>
    <!-- Load custom script last -->
    <script src="~/js/app.bundle.js" asp-append-version="true"></script>
    @{
        string title = (string)ViewData["Title"];
        string subject = (string)ViewData["Subject"];
    }
    <script>
        App.MainSite.load();
    </script>
    @* Load custom scripts *@
    @if (title == "Cars")
    {
        <script>
            App.CarColumnFilter.load();
        </script>
    }
    @if (title == "Leads")
    {
        <script>
            App.LeadColumnFilter.load();
        </script>
    }
    @if ((title == "Edit" || title == "Create") && subject == "Car")
    {
        <script>
            App.CarAjax.load();
        </script>
    }
</body>

</html>